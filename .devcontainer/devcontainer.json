{
  "name": "Ubuntu with RDP",
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu-22.04",
  "build": {
    "dockerfile": "../Dockerfile"
  },
  "forwardPorts": [3389],
  "portsAttributes": {
    "3389": {
      "label": "RDP",
      "onAutoForward": "notify"
    }
  },
    "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode-remote.remote-containers"
      ]
    }
  }
}
